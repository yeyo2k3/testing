<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
"-//Apache Software Foundation//DTD Struts Configuration 2.5//EN"
"http://struts.apache.org/dtds/struts-2.5.dtd">

<struts>
	
	<package name="default" namespace="/" extends="struts-default">
		 <action name="initializeList" 
             class="com.struts.action.ProjectAction" method="initializeList">
			<result name="none">/projectsLocations.jsp</result>
	   </action>
		<action name="actionlistingProjects" class="com.struts.action.ProjectAction" method="save" >
			<result name="success">/projectsLocations.jsp</result>
		</action>
		
		<action name="actionDeleteProjects" class="com.struts.action.ProjectAction" method="delete">
			<result name="success">/projectsLocations.jsp</result>
		</action>
		
		<action name="actionClearProjects" class="com.struts.action.ProjectAction" method="clear">
			<result name="success">/projectsLocations.jsp</result>
		</action>
				
	</package>

</struts>